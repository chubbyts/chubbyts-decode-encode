{
  "name": "@chubbyts/chubbyts-decode-encode",
  "version": "1.0.4",
  "description": "A simple decode/encode solution for json / jsonx / url-encoded / xml / yaml.",
  "keywords": [
    "chubbyts",
    "decode",
    "encode",
    "json",
    "jsonx",
    "url-encoded",
    "xml",
    "yaml"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/chubbyts-decode-encode",
  "scripts": {
    "cs-fix": "prettier --write src tests",
    "cs": "prettier --check src tests",
    "test": "jest",
    "infection": "stryker run",
    "build": "tsc",
    "prepare": "rm -Rf dist && npm run build && npm run cs && npm run test"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100
      }
    }
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@chubbyts/chubbyts-throwable-to-error": "^1.0.0",
    "fast-xml-parser": "^4.0.8",
    "qs": "^6.10.5",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.0.2",
    "@stryker-mutator/jest-runner": "^6.0.2",
    "@types/jest": "^27.5.1",
    "@types/node": "^14",
    "@types/qs": "^6.9.7",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
